# Path to your oh-my-zsh installation.
export ZSH=$HOME/.oh-my-zsh

ZSH_THEME="robbyrussell"

# Uncomment the following line to use case-sensitive completion.
# CASE_SENSITIVE="true"

# Uncomment the following line if you want to disable marking untracked files
# under VCS as dirty. This makes repository status check for large repositories
# much, much faster.
# DISABLE_UNTRACKED_FILES_DIRTY="true"

plugins=(history tmux z ssh-agent)

# User configuration

export PATH="$HOME/bin:/opt/local/bin:opt/local/sbin:/usr/bin:/bin:/usr/sbin:/sbin:/usr/local/bin:/opt/X11/bin:$HOME/dev/lux/bin"

source $ZSH/oh-my-zsh.sh

# You may need to manually set your language environment
export LANG=en_US.UTF-8
export LC_CTYPE=en_US.UTF-8

alias ncs-45='source ~/dev/nso-4.5.5.1/ncsrc'
alias ncs-46='source ~/dev/nso-4.6.2/ncsrc'
alias ncs-47='source ~/dev/nso-4.7/ncsrc'
alias dncs-4='source ~/dev/nso-source/confd-6.6/env.sh'
alias tncs='source ~/dev/nso-source/trunk/env.sh'

export USE_SSL_DIR=/opt/local/

export PATH=$HOME/.local/bin:$PATH:$HOME/dev/haskell/stack-1.5.1-osx-x86_64
export PATH="$HOME/.cargo/bin:$PATH"
source ~/dotfiles/my-zsh

alias hoogle='stack hoogle --'

export EDITOR=vim
export SVN_EDITOR=vim

# export GOPATH="$HOME/dev/go:/Volumes/Fredrik/go"
export PATH="$PATH:$HOME/dev/go/bin"

[ -f ~/.fzf.zsh ] && source ~/.fzf.zsh
